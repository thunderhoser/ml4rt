#!/bin/tcsh

#SBATCH --job-name="plot_eval_by_site_same_plots"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="batch"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
#SBATCH --mem=8G
#SBATCH --time=02:00:00
#SBATCH --array=1-3
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=plot_eval_by_site_same_plots_%A_%a.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_standalone/ml4rt"
set TOP_MODEL_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_models/experiment08"

set DENSE_LAYER_COUNTS=(2 3 3)
set DENSE_LAYER_DROPOUT_RATES=(0.100 0.000 0.000)
set L2_WEIGHTS=(0.0000001000 0.0000031623 0.0000003162)

set dense_layer_count=${DENSE_LAYER_COUNTS[$SLURM_ARRAY_TASK_ID]}
set dense_layer_dropout_rate=${DENSE_LAYER_DROPOUT_RATES[$SLURM_ARRAY_TASK_ID]}
set l2_weight=${L2_WEIGHTS[$SLURM_ARRAY_TASK_ID]}

set dense_layer_count_string=`printf "%d" $dense_layer_count`
set dense_layer_dropout_string=`printf "%.3f" $dense_layer_dropout_rate`
set l2_weight_string=`printf "%.10f" $l2_weight`

set top_model_dir_name="${TOP_MODEL_DIR_NAME}/num-dense-layers=${dense_layer_count_string}_dense-dropout=${dense_layer_dropout_string}_l2-weight=${l2_weight_string}"

set model_file_names={${top_model_dir_name}/model*.h5}
set model_file_name=${model_file_names[$#model_file_names]}
set model_dir_name=`echo $model_file_name | rev | cut -c 4- | rev`

set validation_dir_name="${model_dir_name}/isotonic_regression/validation"

set conus_eval_file_names=("${validation_dir_name}/by_site/santa_barbara_ca/evaluation.nc" "${validation_dir_name}/by_site/bodega_bay_ca/evaluation.nc" "${validation_dir_name}/by_site/forks_wa/evaluation.nc" "${validation_dir_name}/by_site/owego_ny/evaluation.nc" "${validation_dir_name}/by_site/tupper_lake_ny/evaluation.nc" "${validation_dir_name}/by_site/belle_mina_al/evaluation.nc" "${validation_dir_name}/by_site/lamont_ok/evaluation.nc")
set ocean_eval_file_names=("${validation_dir_name}/by_site/barrow_ak_ocean/evaluation.nc" "${validation_dir_name}/by_site/cherskii_russia_ocean/evaluation.nc" "${validation_dir_name}/by_site/tiksi_russia_ocean/evaluation.nc" "${validation_dir_name}/by_site/fino3_tower_north_sea/evaluation.nc" "${validation_dir_name}/by_site/azores_arm_site/evaluation.nc")
set high_arctic_eval_file_names=("${validation_dir_name}/by_site/villum_greenland_land/evaluation.nc" "${validation_dir_name}/by_site/eureka_nu_ocean/evaluation.nc" "${validation_dir_name}/by_site/alert_nu_land/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site1/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site2/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site3/evaluation.nc" "${validation_dir_name}/by_site/sheba/evaluation.nc")
set misc_eval_file_names=("${validation_dir_name}/by_site/juelich_germany/evaluation.nc" "${validation_dir_name}/by_site/ny_alesund_norway_land/evaluation.nc" "${validation_dir_name}/by_site/pallas_sodankyla_russia_land/evaluation.nc")

set conus_eval_dir_name="${validation_dir_name}/by_site/evaluation_conus"
set ocean_eval_dir_name="${validation_dir_name}/by_site/evaluation_ocean"
set high_arctic_eval_dir_name="${validation_dir_name}/by_site/evaluation_high_arctic"
set misc_eval_dir_name="${validation_dir_name}/by_site/evaluation_misc"

python3 -u "${CODE_DIR_NAME}/plot_evaluation.py" \
--input_eval_file_names "${conus_eval_file_names[1]}" "${conus_eval_file_names[2]}" "${conus_eval_file_names[3]}" "${conus_eval_file_names[4]}" "${conus_eval_file_names[5]}" "${conus_eval_file_names[6]}" "${conus_eval_file_names[7]}" \
--line_styles "solid" "solid" "solid" "dashed" "dashed" "dashed" "solid" \
--line_colours "27_158_119" "217_95_2" "117_112_179" "27_158_119" "217_95_2" "117_112_179" "0_0_0" \
--set_descriptions "Santa_Barbara_CA" "Bodega_Bay_CA" "Forks_WA" "Owego_NY" "Tupper_Lake_NY" "Bella_Mina_AL" "Lamont_OK" \
--confidence_level=0.99 \
--use_log_scale=1 \
--plot_by_height=1 \
--output_dir_name="${conus_eval_dir_name}"

python3 -u "${CODE_DIR_NAME}/plot_evaluation.py" \
--input_eval_file_names "${ocean_eval_file_names[1]}" "${ocean_eval_file_names[2]}" "${ocean_eval_file_names[3]}" "${ocean_eval_file_names[4]}" "${ocean_eval_file_names[5]}" \
--line_styles "solid" "solid" "solid" "dashed" "dashed" \
--line_colours "27_158_119" "217_95_2" "117_112_179" "27_158_119" "217_95_2" \
--set_descriptions "Barrow_AK" "Cherskii_RU" "Tiksi_RU" "FINO3" "Azores" \
--confidence_level=0.99 \
--use_log_scale=1 \
--plot_by_height=1 \
--output_dir_name="${ocean_eval_dir_name}"

python3 -u "${CODE_DIR_NAME}/plot_evaluation.py" \
--input_eval_file_names "${high_arctic_eval_file_names[1]}" "${high_arctic_eval_file_names[2]}" "${high_arctic_eval_file_names[3]}" "${high_arctic_eval_file_names[4]}" "${high_arctic_eval_file_names[5]}" "${high_arctic_eval_file_names[6]}" "${high_arctic_eval_file_names[7]}" \
--line_styles "solid" "solid" "solid" "dashed" "dashed" "dashed" "solid" \
--line_colours "27_158_119" "217_95_2" "117_112_179" "27_158_119" "217_95_2" "117_112_179" "0_0_0" \
--set_descriptions "Villum_GL" "Eureka_NU" "Alert_NU" "YOPP1" "YOPP2" "YOPP3" "SHEBA" \
--confidence_level=0.99 \
--use_log_scale=1 \
--plot_by_height=1 \
--output_dir_name="${high_arctic_eval_dir_name}"

python3 -u "${CODE_DIR_NAME}/plot_evaluation.py" \
--input_eval_file_names "${misc_eval_file_names[1]}" "${misc_eval_file_names[2]}" "${misc_eval_file_names[3]}" \
--line_styles "solid" "solid" "solid" \
--line_colours "27_158_119" "217_95_2" "117_112_179" \
--set_descriptions "Juelich_DE" "Ny_Alesund_NO" "Pallas_Sodankyla_FI" \
--confidence_level=0.99 \
--use_log_scale=1 \
--plot_by_height=1 \
--output_dir_name="${misc_eval_dir_name}"

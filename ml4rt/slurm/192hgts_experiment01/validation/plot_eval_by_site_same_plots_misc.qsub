#!/bin/tcsh

#SBATCH --job-name="plot_eval_by_site_same_plots_misc"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="batch"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
#SBATCH --mem=8G
#SBATCH --time=02:00:00
#SBATCH --array=1-4
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=plot_eval_by_site_same_plots_misc_%A_%a.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_standalone_sr/ml4rt"
set TOP_MODEL_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_models/192heights/experiment01"

set DENSE_LAYER_COUNTS=(4 2 3 2)
set DENSE_LAYER_DROPOUT_RATES=(0.0 0.2 0.1 0.2)
set L2_WEIGHTS=(0.0000010000 0.0000010000 0.0000100000 0.0001000000)

set dense_layer_count=${DENSE_LAYER_COUNTS[$SLURM_ARRAY_TASK_ID]}
set dense_layer_dropout_rate=${DENSE_LAYER_DROPOUT_RATES[$SLURM_ARRAY_TASK_ID]}
set l2_weight=${L2_WEIGHTS[$SLURM_ARRAY_TASK_ID]}

set dense_layer_count_string=`printf "%d" $dense_layer_count`
set dense_layer_dropout_string=`printf "%.3f" $dense_layer_dropout_rate`
set l2_weight_string=`printf "%.10f" $l2_weight`

set top_model_dir_name="${TOP_MODEL_DIR_NAME}/num-dense-layers=${dense_layer_count_string}_dense-dropout=${dense_layer_dropout_string}_l2-weight=${l2_weight_string}"

set model_file_names={${top_model_dir_name}/model*.h5}
set model_file_name=${model_file_names[$#model_file_names]}
set model_dir_name=`echo $model_file_name | rev | cut -c 4- | rev`

set validation_dir_name="${model_dir_name}/validation"

set misc_eval_file_names=("${validation_dir_name}/by_site/pallas_sodankyla_russia_land/evaluation.nc" "${validation_dir_name}/by_site/cherskii_russia_ocean/evaluation.nc" "${validation_dir_name}/by_site/tiksi_russia_ocean/evaluation.nc" "${validation_dir_name}/by_site/fino3_tower_north_sea/evaluation.nc" "${validation_dir_name}/by_site/azores_arm_site/evaluation.nc")
set high_arctic_eval_file_names=("${validation_dir_name}/by_site/alert_nu_land/evaluation.nc" "${validation_dir_name}/by_site/sheba/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site1/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site2/evaluation.nc" "${validation_dir_name}/by_site/yopp_arctic_ocean_site3/evaluation.nc")

set misc_eval_dir_name="${validation_dir_name}/by_site/evaluation_misc"
set high_arctic_eval_dir_name="${validation_dir_name}/by_site/evaluation_high_arctic"


python3 -u "${CODE_DIR_NAME}/plot_evaluation.py" \
--input_eval_file_names "${misc_eval_file_names[1]}" "${misc_eval_file_names[2]}" "${misc_eval_file_names[3]}" "${misc_eval_file_names[4]}" "${misc_eval_file_names[5]}" \
--line_styles "solid" "solid" "solid" "dashed" "dashed" \
--line_colours "27_158_119" "217_95_2" "117_112_179" "27_158_119" "217_95_2" \
--set_descriptions "Pallas_Sodankyla_FI" "Cherskii_RU" "Tiksi_RU" "FINO3" "Azores" \
--confidence_level=0.99 \
--use_log_scale=1 \
--plot_by_height=1 \
--output_dir_name="${misc_eval_dir_name}"

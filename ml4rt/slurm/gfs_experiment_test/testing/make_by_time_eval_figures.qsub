#!/bin/tcsh

#SBATCH --job-name="make_by_time_eval_figures"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="batch"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
#SBATCH --time=01:00:00
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=make_by_time_eval_figures_%A.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_standalone/ml4rt"
set MODEL_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_models/gfs/test/trained_with_norm_files"
set TOP_OUTPUT_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4rt_project/gfs_experiment_test"

set evaluation_dir_name="${MODEL_DIR_NAME}/testing/by_time/evaluation/by_zenith_angle"
set output_dir_name="${TOP_OUTPUT_DIR_NAME}/eval_by_time"

python3 -u "${CODE_DIR_NAME}/make_by_time_eval_figure.py" \
--input_evaluation_dir_name="${evaluation_dir_name}" \
--heights_m_agl 96 1000 10016 14976 45842 \
--output_dir_name="${output_dir_name}"
